{
  "name": "edice",
  "version": "0.0.1",
  "description": "eDice - Elm dice game",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/* ; webpack --progress -p --env.production",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "heroku-postbuild": "yarn build",
    "test": "bundlesize",
    "generate-maps": "node ./scripts/generate-maps.js",
    "deploy": "rsync -az --force --delete --progress --exclude-from=rsync_exclude.txt -e \"ssh -p22\" ./dist/ gipsy@54.36.183.181:/var/www/quevic.io/",
    "cowboy": "yarn build && yarn test && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/gipsyking/edice.git"
  },
  "keywords": [
    "edice",
    "elm",
    "dice"
  ],
  "author": "Benjamin Grosse",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://bitbucket.org/gipsyking/edice#readme",
  "bundlesize": [
    {
      "path": "./dist/elm-dice.*.js",
      "maxSize": "100 kB"
    },
    {
      "path": "./dist/*.worker.js",
      "maxSize": "50 kB"
    },
    {
      "path": "./dist/elm-dice-serviceworker.js",
      "maxSize": "1 kB"
    },
    {
      "path": "./dist/elm-dice.*.css",
      "maxSize": "25 kB"
    }
  ],
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "bundlesize": "^0.15.3",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "0.25.0",
    "dialog-polyfill": "^0.4.9",
    "elm": "^0.18.0",
    "elm-css-webpack-loader": "2.0.1",
    "elm-webpack-loader": "3.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "^2.30.1",
    "materialize-css": "0.97.7",
    "mqtt": "^2.1.3",
    "muicss": "0.9.1",
    "postcss-loader": "^1.1.1",
    "postcss-partial-import": "^2.1.0",
    "serviceworker-loader": "^0.1.0",
    "style-loader": "0.13.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "worker-loader": "^1.1.0"
  }
}
